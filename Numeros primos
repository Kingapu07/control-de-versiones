int main () {
      int x = 17;
      bool y = true;
      if (x <= 1) {
        y = false;
      }
      else {
        for (int i = 1; i < x * 2; ++i) {
          if (x % i !=0){
            y = false;
            break;
          }
        }
      }
    if (y == true) {
      printf ("El nÃºmero %d es primo", x);
    }
    else ("el numero %d no es primo", x);
}
